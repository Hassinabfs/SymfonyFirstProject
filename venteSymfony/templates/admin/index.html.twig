{% extends 'base.html.twig' %}

{% block title %}Hello AdminController!
{% endblock %}

{% block body %}


	<table class="table">
		<thead>
			<tr>
				<th>ID</th>
				<th>Email</th>
				<th>Username</th>
				<th>Action</th>

			</tr>
		</thead>
		<tbody>
			{% for user in users %}
				<tr>
					<td>{{ user.id}}</td>
					<td>{{ user.email }}</td>
					<td>{{ user.username }}</td>

				</tr>

			{% endfor %}
		</tbody>


	</table>


{% endblock %}
